<%
let onecolumn = false;
if (Object.prototype.hasOwnProperty.call(config, 'onecolumn')) {
  onecolumn = true;
  delete config.onecolumn;
}
-%>
<% if (onecolumn) { -%>
\begin{figure*}[t]
<% } else { -%>
\begin{figure}[tb]
<% } -%>
\centering
\includegraphics[<%= utils.stringify(config) %>]{<%= url %>}
\caption{<%= caption.replace(/\n/g, '\\protect\\\\%\n') %>}
<%= label %>
<% if (onecolumn) { -%>
\end{figure*}
<% } else { -%>
\end{figure}
<% } -%>
